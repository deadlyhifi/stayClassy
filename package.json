{
  "name": "stayclassy",
  "version": "1.2.2",
  "description": "The simplest JS utility for joining CSS classnames.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Tom de Bruin",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deadlyhifi/stayClassy.git"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "keywords": [
    "css"
  ]
}
